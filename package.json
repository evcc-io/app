{
  "name": "evcc",
  "version": "1.0.4",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "tsc --noEmit && eslint .",
    "format": "prettier --write .",
    "translations": "ts-node ./scripts/load-translations.ts",
    "eas:ios": "eas build -p ios",
    "eas:android:github": "eas build --platform android --profile preview",
    "eas:android:playstore": "eas build -p android"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@expo/config-plugins": "~10.1.1",
    "@expo/metro-runtime": "~5.0.4",
    "@material-symbols/svg-400": "^0.29.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@react-navigation/stack": "^6.3.21",
    "@types/react": "~19.0.10",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "axios": "^1.8.4",
    "base-64": "^1.0.0",
    "expo": "^53.0.20",
    "expo-build-properties": "~0.14.8",
    "expo-dev-client": "~5.2.4",
    "expo-localization": "~16.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18next": "^24.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.1.4",
    "react-native": "0.79.5",
    "react-native-eva-icons": "^1.3.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-native-zeroconf": "^0.13.8",
    "typescript": "~5.8.3",
    "v8-android-jit": "^11.1000.4",
    "react-native-edge-to-edge": "1.6.0",
    "expo-linking": "~7.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@types/react": "~19.0.10"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@ui-kitten/components",
          "react-native-eva-icons",
          "@eva-design/eva",
          "@material-symbols/svg-400",
          "@ui-kitten/eva-icons",
          "base-64",
          "typescript",
          "v8-android-jit"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
